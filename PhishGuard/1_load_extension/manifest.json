{
  "manifest_version": 3,
  "name": "PhishGuard",
  "version": "1.0",
  "description": "Detect phishing websites using ML model",
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}

    